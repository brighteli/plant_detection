<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.tensorflow.lite.examples.classification">

    <uses-sdk />

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/man_1"
        android:label="TreeNetApp"
        android:roundIcon="@mipmap/man_1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.ImageClassification">
        <activity android:name=".OptionScreen"></activity>
        <activity android:name=".optionscreen" />
        <activity android:name=".LibraryActivity" />
        <activity android:name=".register" />
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".LibraryListActivity" />
        <activity
            android:name=".ClassifierActivity"
            android:label="@string/tfe_ic_app_name"
            android:screenOrientation="portrait" />
    </application>

</manifest>